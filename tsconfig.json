{
    "compilerOptions": {
        "lib": [
            "ES2020"
        ],
        "module": "commonjs",
        "target": "ES2020",
        "sourceMap": true,
        // "declaration": true,
        // "declarationMap": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "preserveWatchOutput": true,
        "pretty": false,
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}